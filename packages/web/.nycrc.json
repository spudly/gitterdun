{
  "all": true,
  "extension": [".ts", ".tsx"],
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["**/*.test.ts", "**/*.test.tsx", "tests/**", "node_modules/**"],
  "reporter": ["text", "html", "lcov"],
  "check-coverage": true,
  "branches": 100,
  "functions": 100,
  "lines": 100,
  "statements": 100
}

